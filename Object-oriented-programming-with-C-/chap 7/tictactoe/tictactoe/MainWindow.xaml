<Window x:Class="tictactoe.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:tictactoe"
        mc:Ignorable="d"
        Title="TicTacToe - Three in a row" Height="565.392" Width="639.227" ResizeMode="CanResize">
    <Grid Margin="10,0,0.4,20">
        <local:TTTBoard Margin="10,85,3,10" Width="Auto" Cursor="Hand"/>
        <Label x:Name="lblTime" Content="Label" HorizontalAlignment="Left" Margin="494,10,0,0" VerticalAlignment="Top" Width="125" Height="30"/>
        <Label x:Name="TicTacToe_the_game" Content="TicTacToe the game" HorizontalAlignment="Left" Margin="33,27,0,0" VerticalAlignment="Top" Width="427" Height="53" Cursor="Arrow" ToolTip="Best Game ever" IsEnabled="False" FontSize="24"/>
    </Grid>
</Window>
